@{
    ViewData["Title"] = "List";
}
<div class="page-title">
    <div class="title_left">
        <h3>
            Account
        </h3>
    </div>
</div>
<div class="clearfix"></div>
<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <h2>List<small>Users</small></h2>
                <div class="col-md-3 col-sm-3 col-xs-12 form-group pull-right top_search">
                    <div class="input-group">
                        <input type="text" id="account_keywords" class="form-control" placeholder="Search for...">
                        <span class="input-group-btn">
                            <button class="btn btn-default"  id="search_account" type="button">Go!</button>
                        </span>
                    </div>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="x_content">
                <table id="account-grid"></table>
            </div>
        </div>
    </div>
</div>

@section Scripts{
    <script src="~/js/admin/account/list.js"></script>
}